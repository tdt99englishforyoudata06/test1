        "duration": 2.636
    },
    {
        "text": "CONSIDERING.",
        "start": 360.259,
        "duration": 1.168
    },
    {
        "text": "WE'LL LET THEM ANNOUNCE LEGAL ",
        "start": 360.66,
        "duration": 2.302
    },
    {
        "text": "ACTIONS FROM THEIR END, HOPE TO ",
        "start": 361.527,
        "duration": 2.236
    },
