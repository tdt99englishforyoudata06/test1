        "start": 515.881,
        "duration": 3.704
    },
    {
        "text": "HARSMENT.",
        "start": 519.085,
        "duration": 3.069
    },
    {
        "text": "MORE TO THE POINT, YOUR JUSTICE ",
        "start": 519.652,
        "duration": 4.103
    },
    {
        "text": "SECRETARY OVER THE WEEKEND AT A ",
        "start": 522.221,
        "duration": 3.269
    },
    {
        "text": "RALLY ASKED TENS AND THOUSANDS ",
        "start": 523.822,
        "duration": 3.303
    },
