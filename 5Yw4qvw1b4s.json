[
    {
        "text": "In August 1619,",
        "start": 1.022,
        "duration": 2.085
    },
    {
        "text": "a ship appeared on this horizon,",
        "start": 3.107,
        "duration": 1.877
    },
    {
        "text": "near Point Comfort, Virginia.",
        "start": 4.984,
        "duration": 2.127
    },
    {
        "text": "It carried more than",
        "start": 7.111,
        "duration": 1.168
    },
    {
