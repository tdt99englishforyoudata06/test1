        "start": 766.59,
        "duration": 3.12
    },
    {
        "text": "[Applause]",
        "start": 766.63,
        "duration": 3.08
    },
    {
        "text": "[Music]",
        "start": 771.65,
        "duration": 2.24
    },
    {
        "text": "[Applause]",
        "start": 771.68,
        "duration": 4.95
    },
    {
        "text": "[Music]",
        "start": 773.89,
        "duration": 4.47
    },
    {
        "text": "[Applause]",
        "start": 776.63,
        "duration": 2.64
    },
    {
        "text": "[Music]",
        "start": 778.36,
        "duration": 3.27
    },
    {
        "text": "[Applause]",
        "start": 779.27,
        "duration": 41.209
    },
    {
        "text": "[Music]",
        "start": 781.63,
