    {
        "text": " ",
        "start": 276.14,
        "duration": 2.0
    },
    {
        "text": "Absolute destruction.",
        "start": 278.14,
        "duration": 2.0
    },
    {
        "text": " ",
        "start": 280.14,
        "duration": 2.0
    },
    {
        "text": "Murder",
