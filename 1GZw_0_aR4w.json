        "duration": 6.72
    },
    {
        "text": "london",
        "start": 111.84,
        "duration": 3.72
    },
    {
        "text": "in",
        "start": 114.799,
        "duration": 2.481
    },
    {
        "text": "1972",
        "start": 115.56,
        "duration": 4.76
    },
    {
