        "start": 242.319,
        "duration": 3.521
    },
    {
        "text": "money",
        "start": 245.28,
        "duration": 3.28
    },
    {
        "text": "and politics two reasons to go ahead",
        "start": 245.84,
        "duration": 3.2
    },
    {
        "text": "next",
        "start": 248.56,
        "duration": 2.959
    },
