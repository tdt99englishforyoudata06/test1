    {
        "text": "UKRAINE.",
        "start": 129.896,
        "duration": 1.134
    },
    {
        "text": "I HAVEN'T SEEN MY BOYFRIEND.",
        "start": 130.23,
        "duration": 1.935
    },
    {
        "text": "HE'S WITH THE ARMY.",
        "start": 131.131,
        "duration": 2.969
    },
    {
        "text": "I HAVEN'T SEEN MY PARENTS ",
        "start": 132.265,
        "duration": 3.136
    },
    {
        "text": "BECAUSE MY MOM IS IN WESTERN ",
        "start": 134.2,
        "duration": 3.971
    },
