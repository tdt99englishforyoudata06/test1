        "text": "MY NATIVE CITY OF KHARKIV, WHERE",
        "start": 178.711,
        "duration": 3.671
    },
    {
        "text": "I SPENT 17 YEARS.",
        "start": 181.314,
        "duration": 3.103
    },
    {
        "text": "THEY JUST BOMBARDED IT, ",
        "start": 182.482,
        "duration": 3.403
    },
    {
        "text": "BASICALLY TURNED IT INTO A LAMP ",
        "start": 184.517,
        "duration": 3.704
    },
    {
        "text": "POT, WITH THE HOLD WORLD JUST ",
        "start": 185.985,
        "duration": 3.771
    },
    {
        "text": "PRETENDING NOT SEEING WHAT IS ",
        "start": 188.321,
        "duration": 2.169
    },
    {
        "text": "HAPPENING THERE.",
        "start": 189.856,
        "duration": 4.638
    },
    {
        "text": "WHAT ELSE IS THERE FOR ME TO DO?",
        "start": 190.59,
        "duration": 5.872
    },
    {
        "text": "I DON'T REALLY SEE A CHOICE.",
        "start": 194.594,
        "duration": 4.104
    },
    {
        "text": ">> THE 9-YEAR-OLD YOU MENTIONED,",
        "start": 196.629,
        "duration": 3.537
    },
    {
        "text": "YOU HAVEN'T SEEN HIM SINCE THE ",
        "start": 198.798,
        "duration": 2.035
    },
    {
        "text": "WAR BEGAN.",
        "start": 200.466,
        "duration": 2.737
    },
    {
        "text": "YOU SHARED THIS PICTURE HE DREW.",
        "start": 200.967,
        "duration": 4.071
    },
