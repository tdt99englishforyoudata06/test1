    },
    {
        "text": " ",
        "start": 381.96,
        "duration": 2.0
    },
    {
        "text": " ",
        "start": 383.96,
        "duration": 2.0
    },
    {
        "text": "Are you done?",
        "start": 385.96,
        "duration": 2.0
    },
    {
        "text": " ",
        "start": 387.96,
        "duration": 2.0
    },
    {
        "text": "Put this over the beard.",
        "start": 389.96,
