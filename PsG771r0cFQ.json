        "start": 520.399,
        "duration": 4.56
    },
    {
        "text": "anything",
        "start": 522.479,
        "duration": 2.48
    },
    {
        "text": "waking up i saw myself lying in my bed",
        "start": 525.279,
        "duration": 4.641
    },
    {
        "text": "at home",
        "start": 527.6,
        "duration": 2.32
    },
    {
        "text": "no one really knows who saved me",
        "start": 530.16,
        "duration": 4.64
    },
    {
        "text": "unbelievable",
        "start": 532.839,
        "duration": 5.641
    },
