        "start": 349.44,
        "duration": 4.64
    },
    {
        "text": "on valentine's day",
        "start": 351.759,
        "duration": 5.44
    },
    {
        "text": "his name is jake isn't it",
        "start": 354.08,
        "duration": 7.6
    },
    {
        "text": "oh well it was fine",
        "start": 357.199,
        "duration": 10.641
    },
