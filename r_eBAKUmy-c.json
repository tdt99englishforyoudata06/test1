    },
    {
        "text": "SCHOOL.",
        "start": 55.455,
        "duration": 2.069
    },
    {
        "text": "YESTERDAY HER MOTHER TOLD HER",
        "start": 56.423,
        "duration": 1.868
    },
    {
        "text": "NOT TO GO TO SCHOOL BUT SHE",
        "start": 57.69,
        "duration": 1.769
    },
    {
        "text": "SAID NO I WILL GO TODAY.",
        "start": 58.458,
        "duration": 2.002
    },
    {
        "text": "BUT I WILL NOT GO TOMORROW.",
        "start": 59.626,
        "duration": 2.903
    },
    {
        "text": "SHE TOLD THE TRUTH.",
        "start": 61.161,
        "duration": 4.738
    },
    {
        "text": "AND WE BURIED HER HERE TODAY.",
        "start": 63.163,
        "duration": 5.171
    },
    {
        "text": ">> AFGHANS INTERIOR MINISTRY",
        "start": 66.132,
        "duration": 3.404
    },
    {
        "text": "SAYS A CAR BOMB INITIALLY",
        "start": 68.501,
        "duration": 3.137
    },
