        "start": 216.72,
        "duration": 3.36
    },
    {
        "text": "stick",
        "start": 219.12,
        "duration": 4.479
    },
    {
        "text": "you have become too mischievous",
        "start": 220.08,
        "duration": 7.12
    },
    {
        "text": "aunt polly look behind you",
        "start": 223.599,
        "duration": 6.0
    },
