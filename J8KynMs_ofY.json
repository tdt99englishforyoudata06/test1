    {
        "text": "ENVIRONMENTAL DAMAGE.",
        "start": 102.802,
        "duration": 1.802
    },
    {
        "text": "REMEMBER, PEOPLE IN WEST ",
        "start": 103.369,
        "duration": 2.936
    },
    {
        "text": "VIRGINIA DRINK THAT WATER.",
        "start": 104.671,
        "duration": 3.403
    },
