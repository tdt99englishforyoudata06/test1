[
    {
        "text": "Okay Sam.",
        "start": 0.14,
        "duration": 1.0
    },
    {
        "text": "What is up?",
        "start": 1.14,
        "duration": 2.26
    },
    {
        "text": "I'm very happy because basically it is my\nbirthday.",
        "start": 3.4,
        "duration": 3.749
