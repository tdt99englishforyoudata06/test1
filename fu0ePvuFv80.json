    {
        "text": "Hmmm.",
        "start": 18.48,
        "duration": 1.0
    },
    {
        "text": "Should I go out for a nice dinner?",
        "start": 19.48,
        "duration": 1.0
    },
    {
        "text": "Should I have something else?",
        "start": 20.48,
        "duration": 1.23
