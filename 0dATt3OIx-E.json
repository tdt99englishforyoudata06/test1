        "start": 503.569,
        "duration": 7.474
    },
    {
        "text": "                  ",
        "start": 509.976,
        "duration": 1.801
    },
    {
        "text": "GINSBURG, SAID IT'S NATURAL AND ",
        "start": 511.143,
        "duration": 2.069
    },
