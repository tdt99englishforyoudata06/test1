        "duration": 2.87
    },
    {
        "text": "JUDGES.",
        "start": 390.59,
        "duration": 2.202
    },
    {
        "text": "SHE'LL BE ASKED THAT QUESTION.",
        "start": 390.99,
        "duration": 4.538
    },
