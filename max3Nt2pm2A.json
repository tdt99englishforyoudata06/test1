        "start": 125.52,
        "duration": 4.159
    },
    {
        "text": "ax's cutting head",
        "start": 127.52,
        "duration": 4.16
    },
    {
        "text": "repeated strikes pinch the metal in the",
        "start": 129.679,
        "duration": 3.361
    },
    {
        "text": "center",
        "start": 131.68,
        "duration": 3.44
    },
    {
        "text": "he sprinkles the flux mixture onto the",
        "start": 133.04,
        "duration": 4.24
    },
    {
        "text": "surface of the metal form",
        "start": 135.12,
        "duration": 5.72
    },
    {
        "text": "heats it again and folds it into a",
        "start": 137.28,
        "duration": 4.959
    },
    {
        "text": "c-shape",
        "start": 140.84,
        "duration": 3.8
    },
