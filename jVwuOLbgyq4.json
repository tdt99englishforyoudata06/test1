[
    {
        "text": "Since Fortnite is no longer available",
        "start": 0.058,
        "duration": 2.012
    },
    {
        "text": "in the Google Play store,",
        "start": 2.07,
        "duration": 1.85
    },
    {
        "text": "we will be using the Chrome browser",
        "start": 3.92,
        "duration": 1.63
    },
    {
        "text": "to download it to your phone.",
        "start": 5.55,
        "duration": 1.69
    },
    {
        "text": "Make sure you allow apps\nfrom unknown sources",
        "start": 7.24,
        "duration": 2.54
    },
    {
        "text": "to be installed onto your phone.",
        "start": 9.78,
        "duration": 2.0
    },
    {
        "text": "You can do this by selecting settings.",
        "start": 13.1,
        "duration": 2.642
    },
    {
        "text": "Scroll down and select \"Apps.\"",
        "start": 17.7,
        "duration": 2.093
    },
    {
        "text": "Scroll down and tap the Chrome app.",
        "start": 21.12,
        "duration": 3.083
    },
    {
        "text": "Tap \"Install unknown apps.\"",
        "start": 26.88,
        "duration": 2.463
    },
    {
        "text": "Toggle on \"Allow from this source.\"",
        "start": 30.4,
        "duration": 3.063
    },
    {
        "text": "Go to the Chrome browser.",
        "start": 37.68,
        "duration": 1.87
    },
    {
        "text": "In the search bar, type in \"Fortnite.\"",
        "start": 39.55,
        "duration": 3.235
    },
    {
        "text": "Scroll down to the first search result",
        "start": 43.66,
        "duration": 2.358
    },
    {
        "text": "from Epicgames.com",
        "start": 46.018,
        "duration": 1.692
    },
    {
        "text": "and tap \"Download.\"",
        "start": 47.71,
        "duration": 1.996
    },
    {
        "text": "Tap \"DOWNLOAD\" in the upper\nright corner of the screen.",
        "start": 51.3,
        "duration": 2.917
    },
    {
        "text": "On the next page,",
        "start": 55.56,
        "duration": 1.53
    },
    {
        "text": "tap \"Get it on the Epic Games App.\"",
        "start": 57.09,
        "duration": 3.417
    },
    {
        "text": "Tap \"Download\" in the pop-up.",
        "start": 62.81,
        "duration": 1.623
    },
    {
        "text": "Tap \"Open,\"",
        "start": 66.5,
        "duration": 1.518
    },
    {
        "text": "then tap \"Install.\"",
        "start": 69.26,
        "duration": 1.848
    },
    {
        "text": "Tap \"Open.\"",
        "start": 73.57,
        "duration": 1.425
    },
    {
        "text": "Once the Epic Games app is open,",
        "start": 76.53,
        "duration": 2.77
    },
    {
        "text": "select Fortnite.",
        "start": 79.3,
        "duration": 1.932
    },
    {
        "text": "Tap \"INSTALL.\"",
        "start": 82.49,
        "duration": 1.48
    },
    {
        "text": "Once the download is finished,",
        "start": 87.82,
        "duration": 1.87
    },
    {
        "text": "select \"LAUNCH\" and enjoy your game.",
        "start": 89.69,
        "duration": 3.297
    }
]